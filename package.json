{
  "name": "Isntit",
  "version": "0.0.3",
  "description": "Simple javascript data validation library",
  "main": "dist/isntit.common.js",
  "files": [
    "isntit.js"
  ],
  "scripts": {
    "build": "node build/build.js isntit.js",
    "build:all": "node build/build.js",
    "release": "bash build/release.sh",
    "lint": "eslint src",
    "test": "mocha -R mocha-better-spec-reporter --require should --require babel-register --check-leaks ./test/unit.js",
    "test:all": "mocha -R mocha-better-spec-reporter --require should --require babel-register --check-leaks ./test/*.js ./test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoempsaucisse/isntit.git"
  },
  "keywords": [
    "isntit",
    "validation"
  ],
  "author": "stoempsaucisse@hotmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.8.1",
    "mocha": "^3.1.2",
    "mocha-better-spec-reporter": "^3.0.2",
    "rollup": "^0.36.1",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-replace": "^1.1.0",
    "rollup-watch": "^2.5.0",
    "should": "^11.1.1",
    "uglify-js": "^2.6.2"
  }
